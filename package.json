{
  "name": "generic-snmp",
  "version": "3.0.0",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "postinstall": "husky",
    "format": "prettier -w .",
    "package": "run build && companion-module-build",
    "build": "rimraf dist && run build:main",
    "build:main": "tsc -p tsconfig.build.json",
    "dev": "tsc -p tsconfig.build.json --watch",
    "lint:raw": "eslint",
    "lint": "run lint:raw .",
    "test": "vitest --run"
  },
  "dependencies": {
    "@companion-module/base": "~2.0.0-alpha.0",
    "es-toolkit": "^1.44.0",
    "net-snmp": "^3.26.1",
    "p-queue": "^9.1.0"
  },
  "devDependencies": {
    "@companion-module/tools": "^2.7.1",
    "@types/net-snmp": "^3.23.0",
    "@types/node": "^22.19.13",
    "eslint": "^9.39.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "rimraf": "^6.1.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.1",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": "^22.13"
  },
  "license": "ISC",
  "lint-staged": {
    "*.{css,json,md,scss}": [
      "prettier --write"
    ],
    "*.{ts,tsx,js,jsx}": [
      "yarn lint:raw --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bitfocus/companion-module-generic-snmp.git"
  },
  "prettier": "@companion-module/tools/.prettierrc.json",
  "packageManager": "yarn@4.12.0"
}
